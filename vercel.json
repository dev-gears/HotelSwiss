{
  "framework": "nuxtjs",
  "buildCommand": "npm run build",
  "installCommand": "npm ci --prefer-offline --no-audit --legacy-peer-deps",
  "build": {
    "env": {
      "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
      "NITRO_PRESET": "vercel",
      "NODE_ENV": "production",
      "BASE_URL": "https://hotelswiss.stefanivic.com",
      "API_PATH": "/api",
      "SITE_URL": "https://hotel-swiss-prod.vercel.app"
    }
  },
  "regions": ["iad1"]
}
