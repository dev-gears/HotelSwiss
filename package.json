{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "npm run clean && nuxt build",
    "dev": "nuxt dev --host",
    "generate": "npm run clean && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "powershell -Command \"if (Test-Path '.nuxt') { Remove-Item -Recurse -Force '.nuxt' }; if (Test-Path '.output') { Remove-Item -Recurse -Force '.output' }\"",
    "build:analyze": "npm run clean && nuxt build --analyze",
    "typecheck": "nuxt typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^22.15.17",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/ui": "^3.2.3",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/i18n": "^9.5.4",
    "@nuxtjs/leaflet": "^1.2.6",
    "@nuxtjs/seo": "^3.0.3",
    "@nuxtjs/sitemap": "^7.2.10",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.0",
    "@primevue/forms": "^4.3.4",
    "@primevue/nuxt-module": "^4.3.4",
    "@primevue/themes": "^4.3.4",
    "dotenv": "^16.5.0",
    "leaflet-geosearch": "^4.2.0",
    "nuxt": "^3.17.3",
    "pinia": "^3.0.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.4",
    "unhead": "^2.0.8",
    "vue3-carousel": "^0.15.1"
  },
  "overrides": {
    "vue": "latest"
  }
}
